   "source": [
    "# 暴力递归\n",
    "def printFolders(N):\n",
    "    printProcess(1, N, True)\n",
    "\n",
    "def printProcess(i, N, down):\n",
    "    if i > N:\n",
    "        return\n",
    "    printProcess(i + 1, N, True)\n",
    "    print(\"down\" if down else \"up\")\n",
    "    printProcess(i + 1, N, False)\n",
    "\n",
    "N = 4\n",
    "printFolders(N)"
   ]
